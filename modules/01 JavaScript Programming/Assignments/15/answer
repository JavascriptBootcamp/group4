

function printInformationRecipe(){
    
    var obj={
        name:"Chocolate Cake",
        Serves:2,
        Ingredients:["flour","sugar","baking powder"," eggs"]
    
    }
    
    
    
    console.log(obj.name);
    
    console.log("* Serves :"+ obj.Serves);
    
