var myMovieObj = {
    title : "Top Gun",
    duration: 120,
    stars: ["Tom Cruise", "Kelie Meckgileese", "Meg Rayen"]
};

function movie(){
    var str = "";
    for (var key in myMovieObj) {
        if (Array.isArray(myMovieObj[key]))
            console.log("enter");
        str= str + myMovieObj[key] + " ";
    }
}

console.log(movie());
