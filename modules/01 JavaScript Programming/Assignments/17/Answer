function movie(myMovieObj){
    var str = "";
    for (var key in myMovieObj) {
        if (Array.isArray(myMovieObj[key]))
            console.log("array");
        str = str + myMovieObj[key] + " ";
    }

    return str;
}

var myMovieObj = {
    title : "Top Gun",
    duration: 120,
    stars: ["Tom Cruise", "Kelie Meckgileese", "Meg Rayen"]
};

console.log(movie(myMovieObj));
