var arr = [1, 2, 5, 7];


var x = function(item){
    return item % 2 === 0;
}

var filtered = function myFilter(arr, x) {
    
    var otherArr = [];
    for (var index = 0; index < array.length; index++) {
        otherArr.push(x(arr[index]));
    }

    return otherArr;
};

console.log(filtered);
