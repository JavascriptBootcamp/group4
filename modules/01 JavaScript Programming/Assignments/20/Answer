var arr = [1, 2, 5, 7];


var x = function(item){
    return item % 2 === 0;
}

var filtered = function myFilter(arr, x) {
    
    var otherArr = [];
    for (var index = 0; index < arr.length; index++) {
        if (x(arr[index])) {
            otherArr.push(arr[index]);
        }
    }

    return otherArr;
};

console.log(filtered(arr,x));
