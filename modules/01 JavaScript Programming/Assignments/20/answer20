var arr = [1, 2, 5, 7];


function  callFunction(arr ,  myFilter){

            if (typeof myFilter === 'function'){
                        var result =[];
                    arr.forEach(element => {
                        if(myFilter(element))
                            result.push(element);
                    });

                        return result;
            }

}

var filtered =  callFunction(arr, function(item){
     return item < 5 ;
});

console.log(filtered);
