function Queue () {

    this.queue = [];

    this.add = function (item) {
        this.queue.unshift(item);
    };

    this.remove = function () {
        this.queue.pop();
    };

    this.printAll = function () {
        console.log(this.queue);
    }
};

var queue = new Queue();
queue.add(1);
queue.printAll();
queue.remove();
queue.printAll();
queue.add(1);
queue.add(2);
queue.add(3);
queue.add(4);
queue.printAll();
queue.remove();
queue.printAll();
